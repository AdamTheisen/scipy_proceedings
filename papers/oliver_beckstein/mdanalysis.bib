@article{Best2013,
	Author = {Best, Robert B and Hummer, Gerhard and Eaton, William A},
	Doi = {10.1073/pnas.1311599110},
	File = {:mnt/science/Paper/mendeley.bib/Proceedings of the National Academy of Sciences of the United States of America/2013{\_}Best, Hummer, Eaton{\_}Native contacts determine protein folding mechanisms in atomistic simulations.pdf:pdf},
	Isbn = {1091-6490 (Electronic)$\backslash$n0027-8424 (Linking)},
	Issn = {1091-6490},
